{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Gorillas",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/gorillas",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
