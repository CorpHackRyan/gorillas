{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Gorillas",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/gorillas",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [],
            "externalConsole": true,
            "console": "externalTerminal",
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "preLaunchTask": "Build Gorillas",
            "setupCommands": [
                {
                    "description": "Enable pretty printing",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
